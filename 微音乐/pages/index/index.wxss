.rank-list{
  width:100%;
}

.rank-item{
  width:100%;
  text-align: left;
  height:3rem;
  line-height:3rem;
  border-bottom:1px solid #eee;
  position:relative;
}

.arrow{
  width:10px;
  height:10px;
  border-top:2px solid #999;
  border-right:2px solid #999;
  position:absolute;
  right:20px;
  transform:rotate(45deg);
  top:20px;
}

.text{
  padding-left:1rem;
}

